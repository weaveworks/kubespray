Host *
  User ubuntu

Host 172.16.*
  ProxyCommand ssh -o StrictHostKeyChecking=no -o UserKnownHostsFile=/dev/null  ubuntu@bastion-1711289374.us-east-1.elb.amazonaws.com nc %h %p
  StrictHostKeyChecking no
  UserKnownHostsFile /dev/null
  IdentityFile ~/.ssh/id_rsa
  ControlMaster auto
  ControlPath ~/.ssh/ansible-%r@%h:%p
  ControlPersist 5m
  PreferredAuthentications publickey
  IdentitiesOnly yes
